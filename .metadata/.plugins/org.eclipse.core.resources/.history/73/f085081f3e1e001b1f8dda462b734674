
public class Hello {

	public static void main (String[] args) {
		// TODO Auto-generated method stub
		String str1 = "안녕하세요. 첫 번째 자바 프로그램입니다.";
		String str2 = " 강사님 목소리가 졸려서 눈물이 납니다.";
		System.out.println(str1+str2);
		
		int q = 34;
		int q2 = 7;
		System.out.println(q%q2);	//i를 i2로 나눈 나머지 출력
		
		double i = 1.23456e+2;	//123.456
		double i1 = 1.23456e+3;	//1234.56
		double i2 = 1.23456e+4;	//12345.6
		System.out.println(i);
		System.out.println(i1);
		System.out.println(i2);
		

	}

}
