<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="file">
	
	<!-- fileno.NEXTVAL : fileno(시퀀스명)레코드의 다음 숫자 넣기 -->
	<insert id="addFile" parameterType="spring.model.FileDto">
		insert into fileInfo values(fileno.NEXTVAL, #{OrigName}, #{savePath})
	</insert>
	
</mapper>